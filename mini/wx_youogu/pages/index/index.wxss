/**index.wxss**/
/* view { */
/* 使用主题颜色 */
/* color: var(--themeColor); */
/* } */
swiper image {
  width: 100%;
  vertical-align: bottom;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 30rpx;
}
.index_cate navigator image {
  width: 100%;
}
.index_floor .floor_item {
  font-size: 40rpx;
}
.index_floor .floor_item .title {
  padding: 20rpx 0 0;
}
.index_floor .floor_item .title image {
  width: 100%;
}
.index_floor .floor_item .item_container navigator {
  width: 33.33%;
  float: left;
}
.index_floor .floor_item .item_container navigator:nth-last-child(-n + 4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.index_floor .floor_item .item_container navigator:nth-child(2),
.index_floor .floor_item .item_container navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_item .item_container navigator image {
  width: 100%;
  height: 100%;
}
