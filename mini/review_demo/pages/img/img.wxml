<!--pages/img/img.wxml-->
<text>pages/img/img.wxml</text>
<view wx:for="{{[1, 2, 3, 4, 5, 6, 7, 8, 9]}}" wx:for-item="i" wx:key="index" class="item">
  <view wx:for="{{[1, 2, 3, 4, 5, 6, 7, 8, 9]}}" wx:for-item="j" wx:key="index">
    <view wx:if="{{i <= j}}" >
      {{i}} * {{j}} = {{i * j}}
    </view>
    <view wx:else class="block">
  <!--  &nbsp; -->
  {{ block }}
    </view>
  </view>
</view>

